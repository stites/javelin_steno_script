// Generated from polyglot.javelin-layout

var layer;
func setLayer(layerIndex, layerIdEvent) {
  releaseAll();
  layer = layerIndex;
  sendEvent(layerIdEvent);
}

func dispatchToLayer(layer0Func, layer1Func, layer2Func, layer3Func, layer4Func) {
  if (layer == 0) {
    if (layer0Func) layer0Func();
  } else if (layer == 1) {
    if (layer1Func) layer1Func();
  } else if (layer == 2) {
    if (layer2Func) layer2Func();
  } else if (layer == 3) {
    if (layer3Func) layer3Func();
  } else if (layer == 4) {
    if (layer4Func) layer4Func();
  }
}

